# LeanCloud 配置
# 开发环境
LEANCLOUD_APP_ID_DEV=your_dev_app_id
LEANCLOUD_APP_KEY_DEV=your_dev_app_key
LEANCLOUD_SERVER_URL_DEV=your_dev_server_url
LEANCLOUD_APP_MASTER_KEY_DEV=your_dev_master_key

# 生产环境
LEANCLOUD_APP_ID=your_prod_app_id
LEANCLOUD_APP_KEY=your_prod_app_key
LEANCLOUD_APP_MASTER_KEY=your_prod_master_key

# OpenAI 配置
# 开发环境（可选，如果不需要AI功能可以留空）
OPENAI_API_KEY_PROXY=your_openai_api_key
OPENAI_BASE_URL_PROXY=your_openai_base_url

# 生产环境（可选，如果不需要AI功能可以留空）
OPENAI_API_KEY=your_openai_api_key

# 服务器配置
PORT=13000
NODE_ENV=development
